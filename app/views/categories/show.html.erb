<div class="title_container">
  <h1>Події категорії: "<%= @category.name %>"</h1>
  <%= link_to 'Back', event_params_path, class: 'btn btn-custom-color' %>
</div>
 <table class="table table-striped" id="events-table">
    <thead>
    <tr>
      <th>Назва</th>
      <th>Опис</th>
      <th>Категорія</th>
      <th>Впровадження</th>
      <th>Випуск в маркет</th>
      <th>Дії</th>
    </tr>
  </thead>
    <tbody data-search-target="results">
      <%= render partial: 'events/events_table', locals: { events: @category.events } %>
    </tbody>
  </table>